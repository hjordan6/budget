{
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      },
      {
        "source": "**/*.@(js|css|png|jpg|jpeg|gif|svg|webp)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=86400"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "budget-dev-6465b",
          "appId": "1:84010342754:android:bee572d979f9dcc764f43b",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "budget-21dbe",
          "appId": "1:287130048801:ios:dced6df78cd0acc0d2c660",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "budget-21dbe",
          "configurations": {
            "android": "1:287130048801:android:ef09e101a1391448d2c660",
            "ios": "1:287130048801:ios:dced6df78cd0acc0d2c660",
            "web": "1:287130048801:web:4d5fb4f725e86f0ed2c660"
          }
        },
        "lib/firebase_options_dev.dart": {
          "projectId": "budget-dev-6465b",
          "configurations": {
            "android": "1:84010342754:android:bee572d979f9dcc764f43b",
            "web": "1:84010342754:web:7211d6d4fdcdc09964f43b"
          }
        }
      }
    }
  },
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "disallowLegacyRuntimeConfig": true,
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run build"
      ]
    }
  ]
}